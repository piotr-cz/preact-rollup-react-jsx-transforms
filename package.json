{
  "name": "preact-rollup-react-jsx-transforms",
  "version": "1.0.0",
  "description": "Preact with 3p packages using JSX syntax",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "rollup --config --environment NODE_ENV:development --watch",
    "build": "rollup --config --environment NODE_ENV:production",
    "start:with-workarounds": "rollup --config rollup.config.with-workarounds.js --environment NODE_ENV:development --watch",
    "build:with-workarounds": "rollup --config rollup.config.with-workarounds.config.js --environment NODE_ENV:production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piotr-cz/preact-rollup-react-jsx-transforms.git"
  },
  "author": "Piotr Konieczny <hello@piotr.cz> (https://www.piotr.cz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piotr-cz/preact-rollup-react-jsx-transforms/issues"
  },
  "homepage": "https://github.com/piotr-cz/preact-rollup-react-jsx-transforms#readme",
  "dependencies": {
    "@rest-hooks/rest": "^1.0.4",
    "htm": "^3.0.4",
    "preact": "^10.5.12",
    "rest-hooks": "^5.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-virtual": "^2.0.3",
    "rollup": "^2.40.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-static-files": "^0.2.0"
  }
}
